@model WebUI.Models.ProfileViewModel
@{
    ViewBag.Title = "My Profile";    
}


<div class="row">

    <div class="col-md-12 mb-4">
        <ul class="nav nav-tabs" role="tablist">
            <li class="nav-item">
                <a class="nav-link active show" data-toggle="tab" href="#home3" role="tab" aria-controls="home" aria-selected="true">
                    <i class="icon-user"></i> Verify KYC
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#profile3" role="tab" aria-controls="profile" aria-selected="false">
                    <i class="icon-basket-loaded"></i> Personal Information
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#messages3" role="tab" aria-controls="messages">
                    <i class="icon-pie-chart"></i> Login History
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#change_password" role="tab" aria-controls="messages">
                    <i class="icon-key"></i> Change Password
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#two_factor" role="tab" aria-controls="messages">
                    <i class="icon-key"></i> Two-Factor Authentication
                    <span class="badge badge-pill badge-info">
                       @Model.TwoFactorStatus
                    
                    </span>
                </a>
            </li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane active show" id="home3" role="tabpanel">
                <div style="height:450px"></div>
            </div>
            <div class="tab-pane" id="profile3" role="tabpanel">

                @Html.Partial("_PersonalInformationPartial", Model)
                

            </div>
            <div class="tab-pane" id="messages3" role="tabpanel">
                @Html.Partial("_LoginHistoryPartial", Model)
            </div>
            <div class="tab-pane" id="change_password" role="tabpanel">

                @Html.Partial("_ChangePasswordPartial")
            </div>
            <div class="tab-pane" id="two_factor" role="tabpanel">
                @Html.Partial("_TwoFactorPartial", Model)
            </div>
        </div>
    </div>

</div>

 
